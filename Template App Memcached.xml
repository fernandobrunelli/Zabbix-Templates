                    <name>Memcached: Get status</name>
                    <key>memcached.stats[&quot;{$MEMCACHED.CONN.URI}&quot;]</key>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Zabbix raw items</name>
                        </application>
                    </applications>
