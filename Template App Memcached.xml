                    </preprocessing>
                    <master_item>
                        <key>memcached.stats[&quot;{$MEMCACHED.CONN.URI}&quot;]</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1 and {strlen()}&gt;0</expression>
                            <name>Memcached: Version has changed (new version: {ITEM.VALUE})</name>
                            <priority>INFO</priority>
