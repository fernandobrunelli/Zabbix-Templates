        </trigger>
        <trigger>
            <expression>{Template App Memcached:memcached.connections.current.min(5m)}/{Template App Memcached:memcached.connections.max.last()}*100&gt;{$MEMCACHED.CONN.PRC.MAX.WARN}</expression>
